{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DBConnection": "<your_db_connection_info>"
  },
  "Origins": [
    "<allowed_origins>"
  ],
  "Authentication": {
    "Authority": "<authority_uri>",
    "IssuerUri": "<issuer_uri>",
    "Scopes": [ "<your_scopes>" ]
  },
  "Email": {
    "From": "<email_address>",
    "SmtpServer": "<smtp_server>",
    "Port": 465,
    "Username": "<user_name>",
    "Password": "<password>"
  },
  "Files": {
    "WebRootContentFolder": "media",
    "ContentFolder": "Content",
    "FileSizeLimit": "2097152"
  }
}
