{
    "extends": [
        "airbnb",
        "prettier",
        "plugin:react/recommended"
    ],
    "plugins": [
        "prettier",
        "react"
    ],
    "rules": {
        "prettier/prettier": "error",
        "import/no-cycle": "off",
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "no-nested-ternary": "off",
        "react/jsx-props-no-spreading": [1, { "custom": "ignore" }],
        "no-param-reassign": "off"
    }
}
